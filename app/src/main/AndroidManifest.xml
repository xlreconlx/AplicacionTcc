<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplicacionTCC"
        android:usesCleartextTraffic="true"
        tools:targetApi="31" >
        <activity
            android:name=".cliente.CrearClienteActivity"
            android:exported="false"
            android:label="@string/title_activity_crear_cliente"
            android:theme="@style/Theme.AplicacionTCC" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AplicacionTCC" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".cliente.EditarClienteActivity"
            android:label="@string/editar_cliente_activity_label"
            android:theme="@style/Theme.AplicacionTCC" >
        </activity>
    </application>

</manifest>